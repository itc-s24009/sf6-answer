// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Condition {
  id        String   @id
  text      String
  charas    String[]
  createdAt DateTime @default(now())
}